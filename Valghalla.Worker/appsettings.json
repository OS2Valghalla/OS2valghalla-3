{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Secrets": {
    "Path": "../../../../Environment/secrets.json"
  },
  "Job": {
    "ConnectionString": "<Connectionstring>",
    "ParticipantSyncJob": {
      "ConcurrentLimit": 5,
      "Timeout": 1,
      "BatchSize": 50,
      "Period": 168,
      "Retry": 1,
      "RetryPeriod": 24
    },
    "ElectionActivationJob": {
      "ConcurrentLimit": 5,
      "Timeout": 1,
      "Period": 24,
      "Retry": 1,
      "RetryPeriod": 24
    },
    "ElectionDeactivationJob": {
      "ConcurrentLimit": 5,
      "Timeout": 1,
      "Period": 24,
      "Retry": 1,
      "RetryPeriod": 24
    },
    "TaskNotificationJob": {
      "ConcurrentLimit": 25,
      "Timeout": 1,
      "Retry": 1,
      "RetryPeriod": 24
    },
    "AuditLogClearJob": {
      "MaxAvailableTime": 6,
      "ConcurrentLimit": 5,
      "Timeout": 1,
      "Period": 168,
      "Retry": 1,
      "RetryPeriod": 24
    },
    "CommunicationLogClearJob": {
      "MaxAvailableTime": 4,
      "ConcurrentLimit": 5,
      "Timeout": 1,
      "Period": 168,
      "Retry": 1,
      "RetryPeriod": 24
    }
  },
  "Logger": {
    "Serilog": {
      "TenantPath": "../../../../logs",
      "SystemPath": "../../../../logs/__System__",
      "FileName": "Valghalla.Worker-.log",
      "RollingInterval": "Day",
      "OutputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId} {Level:u3}] {Username} {Message:lj}{NewLine}{Exception}"
    }
  }
}
